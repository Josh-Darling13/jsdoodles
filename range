/**
 * A funtion that accpects 2 parameters
 * start and end, it then returns an
 * array containing all the numbers
 * from start to end.
 * 
 * This new verserion of the this function 
 * is the same as writing:
 * 
 * console.log(sum(range(1,10)));
 * 
 * Adding all the vaules in the array
 * 
 */

 const pickedRange = (start, end) => {
    let arrayRange = [0];
    let arrayTotes = 0;
  
    while (start <= end) {
      arrayRange.push(start);
      start++;
    }
  
    for (let item of arrayRange) {
      let asNum = Number(arrayRange[item]);
      arrayTotes += asNum;
    }
      return arrayTotes;
  };
  
  console.log(pickedRange(1, 10));
  