/**
 * A funtion that accpects 2 parameters
 * start and end, it then returns an
 * array containing all the numbers
 * from start to end.
 *
 * This new verserion of the this function
 * is the same as writing:
 *
 * console.log(sum(range(1,10)));
 *
 * Adding all the vaules in the array
 * 
 * The third argument in the array will take the 
 * value "skip" and find only number divisable by
 * that number and then at the sum of them. 
 *
 */


const basicRange = (start, end) =>{
  let arrayRange = [0];
  while (start <= end) {
    arrayRange.push(start);
    start++;
  }
  return arrayRange;
};

const pickedRange = (start, end, skip) => {
  let basicArray = [0];
  let arrayTotes = 0;

  basicArray = basicRange(start,end);

  for (let item of basicArray) {
    let asNum = Number(basicArray[item]);
    arrayTotes += asNum;
  }

  return arrayTotes;
};

console.log(pickedRange(1, 10));
